extends _master

block head-extra
	link(rel='stylesheet', href='/core/css/app.css')

block head-requirejs
	if (env == 'production')
		script(src='/core/lib/requirejs/js/require.min.js', data-main='/core/js/yy-app')
	else
		script(src='/core/lib/requirejs/js/require.js', data-main='/core/js/yy-app')

block body-content
	div#header-content
	div.full-size#page-content
		div.content
			div#header-background-content
			div#main-content.container-fluid
				div.page-board
					div#left-panel-content.col-sm-5
					div#right-panel-content.col-sm-7