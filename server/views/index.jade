extends _master

block head-extra
	if (env == 'production')
		link(rel='stylesheet', href='/core/css/app.min.css')
	else
		link(rel='stylesheet', href='/core/css/app.css')

block head-requirejs
	if (env == 'production')
		script(src='/core/lib/requirejs/js/require.min.js', data-main='/core/js/app.min')
	else
		script(src='/core/lib/requirejs/js/require.js', data-main='/core/js/app')

block body-content

	div.view-container(ng-view)