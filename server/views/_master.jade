doctype html
html
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")

		title= title

		if (env == 'development')
			link(rel='stylesheet', href='/core/lib/bootstrap3/css/bootstrap.css')
			link(rel='stylesheet', href='/core/lib/bootstrap3/css/bootstrap-theme.css')

			script(src='/core/lib/jquery/js/jquery.js')
			script(src='/core/lib/modernizr/js/modernizr.js')
			script(src='/core/lib/underscore/js/underscore.js')
			script(src='/core/lib/amplify/js/amplify.js')
			script(src='/core/lib/socket.io/js/socket.io.js')
			script(src='/core/lib/angular/js/angular.min.js')
			script(src='/core/lib/jade/js/jade.min.js')

		else if (env == 'production')
			link(rel='stylesheet', href='/core/lib/bootstrap3/css/bootstrap.min.css')
			link(rel='stylesheet', href='/core/lib/bootstrap3/css/bootstrap-theme.min.css')

			script(src='/core/lib/jquery/js/jquery.min.js')
			script(src='/core/lib/modernizr/js/modernizr.min.js')
			script(src='/core/lib/underscore/js/underscore.min.js')
			script(src='/core/lib/amplify/js/amplify.min.js')
			script(src='/core/lib/socket.io/js/socket.io.min.js')
			script(src='/core/lib/angular/js/angular.js')
			script(src='/core/lib/jade/js/jade.min.js')

		block head-extra

		block head-requirejs

	body
		block body-content